<h1>Specifikacionet Per Te Gjithe Eksportet</h1>
<div>
  <center>
    <table border="1">
      <tr>
        <td>
          Numri i Eksportit
        </td>
        <td>
          Date
        </td>
      </tr>
      <% @exports.each do |a| %>
          <tr>
            <td style="text-align:center"><%= a.nr_exportit %></td>
            <td style="text-align:center"><%= a.data %></td>
            <td><%= link_to 'Specifikacioni', specifikacioni_path(a) %></td>
          </tr>
      <% end %>
    </table>
  </center>
</div>