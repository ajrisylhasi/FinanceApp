      </td>
    </tr>
    <% if @client.konsignacioni != nil %>
      <tr style="height: 6	0px">
        <td colspan="4">
        	<center><h1 style="font-family: Barlow; margin-bottom:20px">Konsignacioni: <strong><%= @client.konsignacioni %></strong></h1></center>
        </td>
      </tr>
      <tr>
      	<td>
          <div>
            <strong>Shteti</strong>
          </div>
        </td>
        <td>
